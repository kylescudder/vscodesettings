{
	"name": "netcore-snippets",
	"displayName": "Essential ASP.NET Core 3 Snippets",
	"description": "High quality Code Snippets that boost your ASP.NET Core development productivity.",
	"version": "3.15.1",
	"publisher": "doggy8088",
	"icon": "icon.png",
	"repository": "https://github.com/doggy8088/netcore-snippets",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"*"
	],
	"main": "src/extension",
	"license": "MIT",
	"keywords": [
		".net-core",
		"asp.net-core",
		"code-snippets"
	],
	"extensionPack": [
		"tintoy.msbuild-project-tools"
	],
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"vsce:publish": "vsce publish",
		"vsce:package": "vsce package",
		"vsce:publish:patch": "vsce publish patch",
		"vsce:publish:minor": "vsce publish minor",
		"vsce:publish:major": "vsce publish major"
	},
	"devDependencies": {
		"@types/vscode": "^1.39.0",
		"@types/node": "^10.16.3",
		"ts-node": "^8.4.1",
		"typescript": "^3.6.3",
		"vsce": "^1.66.0"
	},
	"contributes": {
		"snippets": [
			{
				"language": "csharp",
				"path": "./snippets/csharp.json"
			},
			{
				"language": "proto3",
				"path": "./snippets/proto3.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "msbuild",
				"path": "./snippets/msbuild.json"
			},
			{
				"language": "editorconfig",
				"path": "./snippets/editorconfig.json"
			}
		]
	},
	"__metadata": {
		"id": "654caa5d-3ee0-4878-a5e9-fcba9582fa26",
		"publisherId": "752bc7a5-1edc-4491-add8-178f925deaf7",
		"publisherDisplayName": "Will 保哥"
	}
}